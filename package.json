    "enmap": "^5.9.7",
    "enmap-sqlite": "^1.0.2",
    "ms": "^2.1.3",
    "humanize-duration": "^3.31.0",
    "pretty-ms": "^8.0.0",
    "pluralize": "^8.0.0",
    "lodash": "^4.17.21",
    "validator": "^13.11.0",
    "url-regex": "^5.0.0",
    "profanity-js": "^1.0.4",
    "mathjs": "^11.10.1",
    "random": "^3.0.6",
    "cat-ascii-faces": "^2.1.0",
    "anime-quotes": "^1.0.2",
    "animals-js": "^1.0.4",
    "giveaway-js": "^1.0.1",
    "croxydb": "^0.0.22",
    "nekos.life": "^2.2.1",
    "waifu.pics": "^1.0.1",
    "giphy-js-sdk-core": "^1.0.6",
    "tenorjs": "^1.0.8",
    "minecraft-server-util": "^5.3.0",
    "steamapi": "^2.2.0",
    "os-utils": "^0.0.14",
    "systeminformation": "^5.21.9",
    "node-os-utils": "^1.3.7",
    "pm2": "^5.3.0",
    "node-schedule": "^2.1.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.0.3",
    "jsdoc": "^4.0.2",
    "typescript": "^5.2.2",
    "@types/node": "^20.8.10",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "ts-node": "^10.9.1",
    "ts-jest": "^29.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "standard-version": "^9.5.0",
    "conventional-changelog-cli": "^4.1.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "terser-webpack-plugin": "^5.3.9",
    "copy-webpack-plugin": "^11.0.0",
    "dotenv-webpack": "^8.0.1",
    "nyc": "^15.1.0",
    "benchmark": "^2.1.4",
    "faker": "^6.6.6",
    "mock-fs": "^5.2.0",
    "sinon": "^17.0.1",
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "istanbul": "^0.4.5",
    "coveralls": "^3.1.1",
    "codecov": "^3.8.3",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "npm-check-updates": "^16.14.2",
    "depcheck": "^1.4.7",
    "bundlewatch": "^0.3.3",
    "size-limit": "^8.2.6",
    "dockerfile-lint": "^0.3.3",
    "markdownlint-cli": "^0.37.0",
    "typedoc": "^0.25.4",
    "typedoc-plugin-markdown": "^3.16.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "utf-8-validate": "^6.0.3",
    "zlib-sync": "^0.1.9",
    "erlpack": "discord/erlpack",
    "node-canvas-webgl": "^0.2.4-alpha",
    "prism-media": "^1.3.5"
  },
  "peerDependencies": {
    "node-gyp": "^9.0.0"
  },
  "peerDependenciesMeta": {
    "node-gyp": {
      "optional": true
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/",
      "/coverage/",
      "/logs/"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "plugin:jest/recommended"
    ],
    "rules": {
      "no-console": "off",
      "import/no-unresolved": "off",
      "no-underscore-dangle": "off",
      "class-methods-use-this": "off",
      "max-classes-per-file": "off",
      "no-await-in-loop": "off"
    },
    "env": {
      "node": true,
      "jest": true
    }
  },
  "browserslist": [
    "node >= 18"
  ],
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "main.js",
    "config.js",
    "settings.js",
    "commands/",
    "events/",
    "functions/",
    "utils/",
    "schemas/",
    "assets/",
    "locales/",
    "LICENSE",
    "README.md"
  ]
  }
