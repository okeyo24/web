{
  "name": "mayonk-bot",
  "version": "2.5.0",
  "description": "A powerful, modular Discord bot with 321+ plugins and 17+ command categories",
  "main": "main.js",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node main.js",
    "dev": "nodemon main.js",
    "dev:debug": "nodemon --inspect main.js",
    "test": "jest --coverage --verbose",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "node build.js",
    "deploy:commands": "node deploy-commands.js",
    "docker:build": "docker build -t mayonk-bot .",
    "docker:run": "docker run -d --name mayonk-bot --env-file .env mayonk-bot",
    "docker:compose": "docker-compose up -d",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs mayonk-bot",
    "pm2:monitor": "pm2 monit",
    "db:migrate": "node scripts/migrate-database.js",
    "db:backup": "node scripts/backup-database.js",
    "docs:generate": "jsdoc -c jsdoc.json",
    "stats": "node scripts/generate-stats.js",
    "clean": "rm -rf node_modules package-lock.json && npm cache clean --force",
    "preinstall": "npm audit --production",
    "postinstall": "npm run build",
    "prepare": "husky install",
    "precommit": "npm run lint && npm run test",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch"
  },
  "keywords": [
    "discord",
    "bot",
    "moderation",
    "music",
    "economy",
    "automation",
    "utility",
    "discordjs",
    "mayonk",
    "plugins",
    "modular"
  ],
  "author": {
    "name": "Laurie",
    "email": "laurie@mayonk.xyz",
    "url": "https://mayonk.xyz"
  },
  "contributors": [
    {
      "name": "Mayonk Team",
      "email": "team@mayonk.xyz"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okeyo24/web.git"
  },
  "bugs": {
    "url": "https://github.com/okeyo24/web/issues"
  },
  "homepage": "https://mayonk.xyz",
  "funding": {
    "type": "individual",
    "url": "https://ko-fi.com/mayonk"
  },
  "dependencies": {
    "discord.js": "^14.14.1",
    "@discordjs/voice": "^0.16.0",
    "@discordjs/rest": "^2.2.0",
    "@discordjs/builders": "^1.8.0",
    "@discordjs/opus": "^0.9.0",
    "ffmpeg-static": "^5.2.0",
    "libsodium-wrappers": "^0.7.13",
    "ytdl-core": "^4.11.5",
    "play-dl": "^1.9.8",
    "spotify-web-api-node": "^5.0.2",
    "mongoose": "^7.6.3",
    "redis": "^4.6.11",
    "ioredis": "^5.3.2",
    "sqlite3": "^5.1.6",
    "sequelize": "^6.35.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "axios": "^1.6.0",
